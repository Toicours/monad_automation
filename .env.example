# Network Configuration
DEFAULT_NETWORK=monad_testnet
# You can override specific network settings:
# NETWORKS_MONAD_TESTNET_RPC_URL=https://custom-rpc.testnet.monad.xyz/
# NETWORKS_MONAD_MAINNET_RPC_URL=https://custom-rpc.mainnet.monad.xyz/

# Legacy single-network settings (still supported, overrides DEFAULT_NETWORK settings)
MONAD_RPC_URL=https://testnet-rpc.monad.xyz/
MONAD_CHAIN_ID=10143

# Default Wallet Configuration 
# You only need to specify either PRIVATE_KEY or MNEMONIC for each wallet
DEFAULT_WALLET_NAME=main
DEFAULT_PRIVATE_KEY=

# Wallet directory - where wallet files will be stored
WALLET_DIRECTORY=wallets

# Gas Configuration
GAS_LIMIT=3000000
GAS_PRICE=10000000000  # 10 gwei

# Contract Addresses
# Add specific contract addresses as you get them
DEX_ROUTER_ADDRESS=
NFT_MARKETPLACE_ADDRESS=

# API Keys (if needed)
ETHERSCAN_API_KEY=

# Logging
LOG_LEVEL=INFO



